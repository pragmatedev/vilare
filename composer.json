{
  "name": "vilare/theme",
  "description": "Vilare Theme",
  "version": "0.1.4",
  "license": "GPLv3",
  "autoload": {
    "psr-4": {
      "Vilare\\": "app/"
    }
  },
  "require": {
    "php": ">=8.2",
    "symfony/var-dumper": "^7.3.3",
    "rosell-dk/webp-convert": "^2.9.3",
    "illuminate/view": "^12.30.1",
    "illuminate/support": "^12.30.1",
    "illuminate/translation": "^12.30.1",
    "illuminate/validation": "^12.30.1",
    "spatie/ignition": "^1.15.1"
  },
  "scripts": {
    "lint": "phpcs",
    "format": "phpcbf",
    "analyze": "phpstan analyse --memory-limit=2048M"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.13.4",
    "wp-coding-standards/wpcs": "^3.2",
    "phpstan/phpstan": "^2.1.28",
    "szepeviktor/phpstan-wordpress": "^2.0.2",
    "php-stubs/wp-cli-stubs": "^2.12",
    "php-stubs/acf-pro-stubs": "^6.5",
    "php-stubs/woocommerce-stubs": "^9.9.5"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
